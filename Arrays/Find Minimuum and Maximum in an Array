#include<iostream>
using namespace std;

void findMinMax(int arr[], int size){

    int min=arr[0];
    int max=arr[0];

    for(int i=1;i<size;i++){

        if(arr[i]<min){
            min=arr[i];
        }

        if(arr[i]>max){
            max=arr[i];
        }
    }

        cout<<"Minimum element is : "<<min<<endl;
        cout<<"Maximum element is : "<<max<<endl;   
}

int main(){

    int arr[5]={5,1,3,4,9};
    int size=5;

    findMinMax(arr, 5);
    return 0;
}
